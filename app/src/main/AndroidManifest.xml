<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="mcmahon.james.scorekeeper">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/Label_app_name"
        android:roundIcon="@mipmap/plus_one_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="mcmahon.james.scorekeeper.SelectNumPlayersActivity"
            android:label="@string/Label_app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="mcmahon.james.scorekeeper.RecordScoresActivity"
            android:label="@string/Label_Record_Scores" />
        <activity
            android:name="mcmahon.james.scorekeeper.CustomNamePlayersActivity"
            android:label="@string/Label_Custom_Name_Players" />
    </application>
</manifest>
